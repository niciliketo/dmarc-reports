<h1>Policy publisheds</h1>

<p style="color: green"><%= notice %></p>

<%= pie_chart PolicyPublished.group(:domain).count %>
<%= line_chart PolicyPublished.group(:domain).joins(:reports).group_by_day('reports.begin').count %>

<div id="policy_publisheds">
  <% @policy_publisheds.each do |policy_published| %>
    <%= render policy_published %>
    <p>
      <%= link_to "Show this policy published", policy_published %>
    </p>
  <% end %>
</div>

<%= link_to "New policy published", new_policy_published_path %>
